{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'ResultDetail',\n  data() {\n    return {\n      result: null\n    };\n  },\n  methods: {\n    fetchDetail(id) {\n      axios.get(`/api/detect/history/${id}`).then(res => {\n        this.result = res.data;\n      }).catch(err => {\n        console.error('获取检测详情失败:', err);\n      });\n    },\n    goBack() {\n      this.$router.push({\n        name: 'History'\n      });\n    }\n  },\n  created() {\n    const id = this.$route.params.id;\n    if (id) {\n      this.fetchDetail(id);\n    } else {\n      console.warn('未传入检测记录ID，无法获取详情');\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","result","methods","fetchDetail","id","get","then","res","catch","err","console","error","goBack","$router","push","created","$route","params","warn"],"sources":["D:\\web\\qianrushi\\my-first-vue-app\\src\\views\\ResultDetail.vue"],"sourcesContent":["<template>\r\n  <div class=\"detail-page\">\r\n    <div v-if=\"result\" class=\"result-box\">\r\n      <h2>📋 检测详情：</h2>\r\n      <p><strong>部件名称：</strong>{{ result.partName }}</p>\r\n      <p><strong>检测时间：</strong>{{ result.detectTime }}</p>\r\n      <p><strong>检测结果：</strong>{{ result.result }}</p>\r\n      <p><strong>置信度：</strong>{{ result.confidence }}%</p>\r\n      <button class=\"back-btn\" @click=\"goBack\">⬅ 返回历史记录</button>\r\n    </div>\r\n    <div v-else class=\"waiting\">\r\n      🔍 正在加载检测详情...\r\n    </div>\r\n    <div class=\"background-decor\"></div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  name: 'ResultDetail',\r\n  data() {\r\n    return {\r\n      result: null\r\n    }\r\n  },\r\n  methods: {\r\n    fetchDetail(id) {\r\n      axios.get(`/api/detect/history/${id}`)\r\n          .then(res => {\r\n            this.result = res.data\r\n          })\r\n          .catch(err => {\r\n            console.error('获取检测详情失败:', err)\r\n          })\r\n    },\r\n    goBack() {\r\n      this.$router.push({name: 'History'})\r\n    }\r\n  },\r\n  created() {\r\n    const id = this.$route.params.id\r\n    if (id) {\r\n      this.fetchDetail(id)\r\n    } else {\r\n      console.warn('未传入检测记录ID，无法获取详情')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.detail-page {\r\n  position: absolute;\r\n  top: 60px; /* 避开导航栏 */\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  background: linear-gradient(to right, #232526, #414345);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  padding: 40px 20px;\r\n  box-sizing: border-box;\r\n  overflow: auto;\r\n}\r\n\r\n.result-box {\r\n  background: rgba(0, 0, 0, 0.3);\r\n  border: 1px solid #00eaff55;\r\n  padding: 30px;\r\n  border-radius: 12px;\r\n  box-shadow: 0 0 20px rgba(0, 234, 255, 0.1);\r\n  text-align: left;\r\n  max-width: 600px;\r\n  width: 100%;\r\n}\r\n\r\n.result-box h2 {\r\n  font-size: 24px;\r\n  margin-bottom: 20px;\r\n  color: #00eaff;\r\n}\r\n\r\n.result-box p {\r\n  font-size: 18px;\r\n  margin: 10px 0;\r\n  line-height: 1.5;\r\n  color: #e0f7ff;\r\n}\r\n\r\n.back-btn {\r\n  margin-top: 30px;\r\n  padding: 10px 20px;\r\n  background-color: #00eaffaa;\r\n  border: none;\r\n  border-radius: 8px;\r\n  font-size: 16px;\r\n  color: #000;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.back-btn:hover {\r\n  background-color: #00d4ff;\r\n}\r\n\r\n.waiting {\r\n  font-size: 20px;\r\n  color: #ccc;\r\n  font-style: italic;\r\n  text-align: center;\r\n}\r\n\r\n.background-decor {\r\n  position: absolute;\r\n  bottom: -100px;\r\n  left: -150px;\r\n  width: 400px;\r\n  height: 400px;\r\n  background: radial-gradient(circle at center, rgba(0, 234, 255, 0.3), transparent);\r\n  filter: blur(120px);\r\n  pointer-events: none;\r\n  user-select: none;\r\n}\r\n</style>\r\n"],"mappings":";AAkBA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE;IACV;EACF,CAAC;