template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  name: 'HistoryPage',\r\n  data() {\r\n    return {\r\n      history: []\r\n    }\r\n  },\r\n  created() {\r\n    this.fetchHistory()\r\n  },\r\n  methods: {\r\n    fetchHistory() {\r\n      axios.get('/api/detect/history')\r\n          .then(res => {\r\n            this.history = res.data\r\n          })\r\n          .catch(err => {\r\n            console.error('获取历史失败', err)\r\n            this.history = [\r\n              {id: 1, partName: '螺栓-A1', time: '2025-05-17 14:00', result: '正常'},\r\n              {id: 2, partName: '阀门-B2', time: '2025-05-17 15:30', result: '裂纹'}\r\n            ]\r\n          })\r\n    },\r\n    goToDetail(id) {\r\n      this.$router.push({name: 'ResultDetail', params: {id}})\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* 你的样式保持不变 */\r\n</style>\r\n"],"mappings":";AA0BA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,YAAY,CAAC;EACpB,CAAC;EACDC,OAAO,EAAE;IACPD,YAAYA,CAAA,EAAG;MACbL,KAAK,CAACO,GAAG,CAAC,qBAAqB,EAC1BC,IAAI,CAACC,GAAE,IAAK;QACX,IAAI,CAACN,OAAM,GAAIM,GAAG,CAACP,IAAG;MACxB,CAAC,EACAQ,KAAK,CAACC,GAAE,IAAK;QACZC,OAAO,CAACC,KAAK,CAAC,QAAQ,EAAEF,GAAG;QAC3B,IAAI,CAACR,OAAM,GAAI,CACb;UAACW,EAAE,EAAE,CAAC;UAAEC,QAAQ,EAAE,OAAO;UAAEC,IAAI,EAAE,kBAAkB;UAAEC,MAAM,EAAE;QAAI,CAAC,EAClE;UAACH,EAAE,EAAE,CAAC;UAAEC,QAAQ,EAAE,OAAO;UAAEC,IAAI,EAAE,kBAAkB;UAAEC,MAAM,EAAE;QAAI,EACnE;MACF,CAAC;IACP,CAAC;IACDC,UAAUA,CAACJ,EAAE,EAAE;MACb,IAAI,CAACK,OAAO,CAACC,IAAI,CAAC;QAACnB,IAAI,EAAE,cAAc;QAAEoB,MAAM,EAAE;UAACP;QAAE;MAAC,CAAC;IACxD;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}